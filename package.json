{
  "name": "melobeem",
  "private": true,
  "scripts": {
    "eslint.autofix": "eslint --ext .jsx --ext .js --fix app/javascript",
    "eslint": "eslint --ext .jsx --ext .js app/javascript",
    "test": "yarn eslint && yarn test.jest",
    "start": "webpack-dev-server",
    "test.jest": "jest app/javascript",
    "stylelint": "stylelint app/javascript/**/*.less"
  },
  "dependencies": {
    "@babel/preset-react": "^7.12.10",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "5.2.1",
    "ahooks": "^2.9.6",
    "axios": "^0.21.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "^5.2.0",
    "swiper": "^6.4.5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>app/javascript/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/javascript/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "version": "0.1.0",
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": ">=4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "prettier": "2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.1",
    "webpack-dev-server": "^3.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.less": "stylelint --fix"
  }
}
